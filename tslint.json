{
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "rules": {
        "interface-name": [true, "never-prefix"],
        "object-literal-sort-keys": false,
        "quotemark": ["single"],
        "variable-name": ["check-format", "allow-leading-underscore"]
    }
}
